Package: lava
Type: Package
Title: Latent Variable Models
Version: 1.6.1
Date: 2018-03-23
Authors@R: c(person("Klaus K.", "Holst", email="klaus@holst.it", role=c("aut", "cre")),
             person("Brice", "Ozenne", role = "ctb"),
