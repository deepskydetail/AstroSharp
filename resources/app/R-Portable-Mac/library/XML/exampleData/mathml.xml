<?xml version="1.0"?>
<!-- x2 + 4x + 4 =0 -->
<!DOCTYPE mrow SYSTEM "mathml.dtd" [
   <!ENTITY invisibletimes "invisible">
]>
<mrow>
         <mrow>
               <msup> <mi>x</mi> <mn>2</mn> </msup> <mo>+</mo>
                   <mrow>
                     <mn>4</mn>
                     <mo>&invisibletimes;</mo>
                     <mi>x</mi>
                   </mrow>
           <mo>+</mo>
           <mn>4</mn>
         </mrow>
           <mo>=</mo>
           <mn>0</mn>
</mrow>
