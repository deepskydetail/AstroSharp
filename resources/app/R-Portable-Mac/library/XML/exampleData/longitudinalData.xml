<?xml version="1.0" ?>
<!DOCTYPE dataframe SYSTEM "RS.dtd">
<!-- A made up example -->
<rs:dataframe xmlns:rs="http://www.omegahat.net/RSXML" 
  xmlns:lgtdl="http://www.r-project.org/XML/lgtdl"
  numRecords="3"
  numVariables = "2" > 
<rs:variables> 
   <variable> Gender </variable> 
   <variable type="lgtdl" > History </variable>
</rs:variables>
<rs:record length="2">
   <rs:element> <character> "MALE" </character> </rs:element>
   <rs:element> 
      <lgtdl:lgtdl>
         <lgtdl:times> <array><integer>1</integer><integer>6</integer>
                     </array></lgtdl:times> 
         <lgtdl:cov> <array><double>100.4</double><double>25.2</double>
                   </array></lgtdl:cov>
      </lgtdl:lgtdl>
   </rs:element>
</rs:record>
<rs:record>
   <rs:element> <character> "FEMALE" </character> </rs:element>
   <rs:element>
      <lgtdl:lgtdl>
         <lgtdl:times><array><integer>1</integer><integer>6</integer>
                        </array></lgtdl:times>
         <lgtdl:cov><array><double>100.4</double><double>25.2</double>
                        </array></lgtdl:cov>
      </lgtdl:lgtdl>
   </rs:element>
</rs:record>
<rs:record>
   <rs:element> <character> "NA" </character> </rs:element>
   <rs:element>
      <lgtdl:lgtdl>
         <lgtdl:times><array><integer>1</integer><integer>6</integer>
                      </array></lgtdl:times>
         <lgtdl:cov><array><double>100.4</double><double>25.2</double>
                      </array></lgtdl:cov>
      </lgtdl:lgtdl>
   </rs:element>
</rs:record>
</rs:dataframe>
