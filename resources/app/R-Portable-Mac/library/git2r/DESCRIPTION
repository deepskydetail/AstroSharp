        'bundle_r_package.r' 'checkout.r' 'commit.r' 'config.r'
        'contributions.r' 'credential.r' 'diff.r' 'fetch.r' 'git2r.r'
        'index.r' 'libgit2.r' 'merge.r' 'note.r' 'odb.r' 'plot.r'
